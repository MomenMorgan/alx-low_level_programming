#include "main.h"
#include <stdlib.h>

 /**
 * array_range - print rang from min to max
 * @min: input
 * @max: input
 * Return: 0
 */

int *array_range(int min, int max)
{
	int *x;
	int i;

	if (min > max)
	return (NULL);

	x = malloc(sizeof(int) * max + 1);
	if (x == NULL)
		return (NULL);

	i = 0;
	while (min <= max)
	{
		x[i] = min;
		min++;

		return (x);
}
